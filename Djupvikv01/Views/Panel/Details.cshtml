@model Djupvikv01.Models.ArticleModel
 

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}




<div class="jumbotron mt-1">
    <div>
        <h4>  @Html.DisplayFor(model => model.rubrik)</h4>
        <hr />

        <p class="bg-light p-2">

            <img class="rounded" src="@Html.DisplayFor(model => model.bild)d" alt="Jordgubbe" width="100" />
        </p>
        <p class="bg-light p-2">
            @Html.DisplayFor(model => model.sammanfattning)
        </p>
        <p class="bg-light p-2">


          
            @Html.Raw(Model.innehall)

        </p>

        <p class="bg-light p-2">
            @Html.DisplayFor(model => model.datum)

        </p>
        <p class="bg-warning p-2">
            <a href="/Panel/"> <i class="fas fa-arrow-left"></i> Tillbaka </a>
        </p>
    </div>
</div>
